<% include partials/header %>

<form action="/login" method="POST">
  <h2 class="container">Login</h2>
  <input
    type="text"
    name="username"
    placeholder="Username"
    class="form-control"
  />
  <br />
  <input
    type="text"
    name="password" 
    placeholder="Password"
    class="form-control"
  />
